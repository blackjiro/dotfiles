#!/usr/bin/env fish

{{ template "set_brew_path.sh.tmpl" . }}

if ! type fisher > /dev/null 2>&1; then
  echo "Installing fisher"
  fish
  curl -sL https://git.io/fisher | bash -c 'source /dev/stdin && fisher install jorgebucaran/fisher'
fi

# .config/fish/fish_plugins hash: {{ include "dot_config/private_fish/fish_plugins" | sha256sum }}

fisher update

